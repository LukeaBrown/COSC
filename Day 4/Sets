s = {1,2,3,4,5}
s
{1, 2, 3, 4, 5}

______________________________Add to Set______________________________

s.add(100)
s
{1, 2, 3, 4, 5, 100}
s.add(0)
s
{0, 1, 2, 3, 4, 5, 100}
s.add(99)
s
{0, 1, 2, 3, 4, 5, 100, 99}

______________________________Ceck for Item in Set______________________________

2 in s
True

65 in s
False

______________________________---______________________________

s.remove(99)
s
{0, 1, 2, 3, 4, 5, 100}

______________________________Union______________________________

s = {1,2,3,4,5}
t = {5,7,8,9}
s.union(t)
{1,2,3,4,5,7,8,9}

______________________________Difference______________________________

s = {1,2,3,4,5}
t = {5,7,8,9}
s.difference(t)
{1,2,3,4}

______________________________Similarities______________________________

s = {1,2,3,4,5}
t = {5,7,8,9}
s.intersection(t)
{5}

